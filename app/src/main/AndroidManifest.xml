<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capstoneappdraft">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GoogleProfileActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstPageActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".LeaderBoardActivity" />
        <activity
            android:name=".LoginActivity"
            android:noHistory="true" />
        <activity android:name=".HomepageActivity" />
        <activity android:name=".MaintenanceRecordsActivity" />
        <activity android:name=".ScoreboardActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".CreateMaintenanceRecordActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>